<section id="newUser">

  <header class="page-header">
    <h1 style="color: green">Login!</h1>
  </header>
  <div>
    <%= form_tag '/login' do |f| %>

      <p>Email:</p> <p><%= text_field_tag :email %></p>
      <p>Password:</p> <p><%= password_field_tag :password %></p>
      <div style="float: center" id="newUser">
        <p><%= submit_tag "Submit", class: "btn btn-success" %></p>
      </div>
    <% end %>
  </div>
</section>